{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": false,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "baseUrl": "src",
    "paths": {
      "/*": ["*"]
    }
  },
  "exclude": ["node_modules"],
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}